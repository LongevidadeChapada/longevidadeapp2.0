{
  "expo": {
    "name": "longevidade com Qualidade",
    "slug": "longevidadeApp",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#cbddd1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.coguudev.longevidade",
      "permissions": ["CAMERA"],
      "versionCode": 2
      
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
